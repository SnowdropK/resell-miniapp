<view class="orderDetailBox"><common-container vue-id="0e159d22-1" title="订单详情" bind:__l="__l" vue-slots="{{['default']}}"><view class="content"><view class="orderStatusBox box"><view class="title">{{ORDER_STATUS_TEXT_MAP[orderDetail.orderStatus]||""}}</view><view class="tip">{{orderDetail.orderStatusDesc}}</view><block wx:if="{{orderDetail.consignee&&orderDetail.mobile}}"><view class="userInfo">{{''+orderDetail.consignee+"\n          "+orderDetail.mobile+''}}</view></block><block wx:if="{{orderDetail.address}}"><view class="address">{{''+orderDetail.address+''}}</view></block><block wx:if="{{AGREEMENT_TYPE_MAP[orderDetail.agreementType||'']}}"><view data-event-opts="{{[['tap',[['goToAttachUrl',['$event']]]]]}}" bindtap="__e">{{''+(AGREEMENT_TYPE_MAP[orderDetail.agreementType||""]||"放原平台赏袋")+''}}</view></block></view><view class="skuSumFeeBox"><view class="skuInfoBox"><image class="mainImg" mode="aspectFill" src="{{orderDetail.mainUrl}}"></image><view class="skuInfo"><view class="title">{{orderDetail.goodsTitle}}</view><view class="feeBox"><view style="text-align:right;">{{"￥"+$root.m0}}</view><view style="text-align:right;">{{"x"+orderDetail.goodsNum}}</view></view></view></view><view class="skuFee"><view class="item"><view class="left">商品价格</view><view class="right">{{'￥'+$root.m1+''}}</view></view><view class="item"><view class="left">运费</view><view class="right">{{"¥"+$root.m2}}</view></view><view class="item"><view class="left sumNum">实付款<text class="tip">（在担保账户中）</text></view><view class="right sumPrice">￥<text class="sumPriceText">{{$root.m3}}</text></view></view></view></view><view class="orderInfoBox box"><view class="title">订单信息</view><view class="item">{{'订单编号：'+orderDetail.orderId+''}}<view data-event-opts="{{[['tap',[['copyOrderId',['$event']]]]]}}" class="copy" bindtap="__e">复制</view></view><view class="item">{{"下单时间："+orderDetail.orderCreateTime}}</view><view class="item">{{"支付时间："+orderDetail.payTime}}</view><block wx:if="{{orderDetail.orderShipTime}}"><view class="item">{{"发货时间："+orderDetail.orderShipTime}}</view></block><block wx:if="{{orderDetail.orderDoneTime}}"><view class="item">{{"交易完成时间："+orderDetail.orderDoneTime}}</view></block><block wx:if="{{orderDetail.refundTime}}"><view class="item">{{"退款时间："+orderDetail.refundTime}}</view></block></view></view></common-container></view>